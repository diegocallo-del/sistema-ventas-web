#!/bin/sh

# Licensed to the Apache Software Foundation (ASF) under one or more contributor license agreements.

# Basic mvnw script placeholder. Replace with full Spring Boot mvnw if needed.

basedir=$(dirname "$(readlink -f "$0")")

MAVEN_OPTS="$MAVEN_OPTS -Xmx1024m"

if [ -x "$basedir/mvnw.bin" ]; then

    exec "$basedir/mvnw.bin" "$@"

else

    MAVEN_PROJECTBASEDIR="$basedir"; export MAVEN_PROJECTBASEDIR

    exec "mvn" "$@"

fi
